                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with main1 as (select ci.*,cg.created changedate,organisation,industry,product,
d.filter_id,d.id issueid,d.issue_key,d.created,
extract(year from cg.created)*10000000000+extract(month from cg.created)*100000000+extract(day from cg.created)*1000000+
 substr(to_char(cg.created,'DD-MON-YY HH24:MI:SS'),-8,2)*10000+substr(to_char(cg.created,'DD-MON-YY HH24:MI:SS'),-5,2)*100
 +substr(to_char(cg.created,'DD-MON-YY HH24:MI:SS'),-2,2) changetime,
extract(year from cg.created)*100 + extract(month from cg.created) yearmonthnum
from (select * from defectdata where REGEXP_LIKE(filter_id, '^[[:digit:]]+$')) d 
left outer join changegroup CG on cg.issueid = d.id 
left outer join changeitem ci on ci.groupid = cg.id
right outer join quality_metrics Q on Q.ext_defect_backlog_filter = d.filter_id
where extract(year from cg.created)*100+extract(month from cg.created)&gt; 
  extract(year from add_months(sysdate,-45))*100+extract(month from add_months(sysdate,-45)))


select distinct
T2.ORGANISATION,
T2.INDUSTRY,
T2.PRODUCT,
T2.FILTER_ID,
T2.ISSUEID,
T2.ISSUE_KEY,
T2.CREATED,
T2.YEARMONTHNUM,
case when T2.STATUS is null then d.status else t2.status end status,
T2.RESOLVED_DATE,
T2.VERIFIED_DATE,
T2.RESOLUTION,
T2.RESOLVED,
case when T2.ASSIGNEE is  null then d.assignee else T2.ASSIGNEE end assignee,
case when T2.PRIORITY is  null then d.priority else T2.PRIORITY end priority ,
case when T2.SEVERITY is  null then d.severity else T2.SEVERITY end severity from 
(select organisation,industry,product,filter_id,issueid,issue_key,created,YEARMONTHNUM,
LAST_VALUE(status IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) status
,LAST_VALUE(date_resolved IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) resolved_DATE
,LAST_VALUE(date_verified IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) VERIFIED_DATE
,LAST_VALUE(resolution IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) resolution
,LAST_VALUE(resolved IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) resolved
,LAST_VALUE(assignee IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) assignee
,LAST_VALUE(priority IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) priority
,LAST_VALUE(severity IGNORE NULLS) OVER (PARTITION BY ISSUE_KEY ORDER BY YEARMONTHNUM
ROWS BETWEEN unbounded preceding and current row) severity from (select organisation,industry,product,filter_id,issue_key,created,
issueid,yearmonthnum,LISTAGG(status,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum)as status,
to_date(TO_CHAR(LISTAGG(DATE_RESOLVED,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum)),'DD-MON-YYYY') date_resolved,
to_date(LISTAGG(DATE_VERIFIED,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum),'DD-MON-YYYY') DATE_VERIFIED,
LISTAGG(resolution,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum)resolution,
to_date(LISTAGG(resolved,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum),'DD-MON-YYYY')resolved,
LISTAGG(assignee,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum)assignee,
LISTAGG(severity,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum)severity,
LISTAGG(priority,',') WITHIN GROUP (ORDER BY ISSUEID,yearmonthnum)priority
from (select * from (select
organisation,industry,product,filter_id,issue_key,created,
issueid,yearmonthnum,case when field = 'status' then newstring end as status,
case when field = 'Date Resolved' then to_date(to_char(to_date(REGEXP_REPLACE(newvalue, '[^0-9A-Za-z]', '') ,'yyyymmdd'),'mm/dd/yyyy'),'mm/dd/yyyy') end as DATE_RESOLVED,
case when field = 'Date Verified' then to_date(to_char(to_date(REGEXP_REPLACE(newvalue, '[^0-9A-Za-z]', '') ,'yyyymmdd'),'mm/dd/yyyy'),'mm/dd/yyyy') end as DATE_VERIFIED,
case when field = 'resolution' then newstring end as resolution,
case when field = 'resolution' then to_date(changedate,'DD-MON-YYYY') end as resolved,
case when field = 'assignee' then newstring end as assignee,
case when field = 'Severity' then newstring end as severity,
case when field = 'Priority' then newstring end as priority,
rank() over (partition by issueid,field,yearmonthnum order by changetime desc)rank1
from main1) where rank1 =1)
group by organisation,industry,product,filter_id,issue_key,created,
issueid,yearmonthnum) T1) T2
left outer join defectdata d on d.issue_key = T2.issue_key and t2.filter_id = d.filter_id</property>
